#include <stdio.h>
 
int main()
{
    int marks;
    printf("Enter the Number of student:-");
    scanf("%d",&marks);
    if(marks >= 90)
    {
    	printf("Grade A");
	}
	else if(marks >= 80)
    {
    	printf("Grade B");
	}
	else if(marks >= 70)
    {
    	printf("Grade C");
	}
	else if(marks >= 60)
    {
    	printf("Grade D");
	}
	else if(marks >= 40)
    {
    	printf("Grade E");
	}
	else 
    {
    	printf("Fail ! better luck next time");
	} 
    return 0;
}
